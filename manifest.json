{
    "manifest_version": 3,
    "name": "Video Rater",
    "version": "1.0",
    "description": "Rate videos speed-play",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}